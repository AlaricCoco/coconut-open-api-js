{
  "name": "open-api-sdk",
  "version": "0.0.0",
  "description": "Coconut Calendar JS Open API SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coconutcalendar/coconut-open-api-js.git"
  },
  "keywords": [
    "coconut calendar",
    "js",
    "javascript",
    "open api",
    "api",
    "sdk"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coconutcalendar/coconut-open-api-js/issues"
  },
  "homepage": "https://github.com/coconutcalendar/coconut-open-api-js#readme",
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "typescript": "^3.1.6"
  }
}
